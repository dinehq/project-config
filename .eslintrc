{
  "extends": ["plugin:react-hooks/recommended", "plugin:prettier/recommended"],
  "plugins": ["simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "printWidth": 120,
        "semi": false,
        "singleQuote": true,
        "bracketSameLine": true
      }
    ]
  }
}
